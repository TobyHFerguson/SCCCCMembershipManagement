{
  "name": "sccccmanagement",
  "version": "1.0.0",
  "description": "Management utilities for SCCCC written in GAS",
  "main": "index.js",
  "scripts": {
    "test": "npx clasp-env --folder . --scriptId 1m11ZhZl2TxlXkNKwvSfhqkO57ZBgu_KdfLUYIV1Lzksa64kiqQ32x5xA && clasp push && clasp run test",
    "push:watch": "npx clasp-env --folder . --scriptId 1m11ZhZl2TxlXkNKwvSfhqkO57ZBgu_KdfLUYIV1Lzksa64kiqQ32x5xA && clasp push -w",
    "push": "npx clasp-env --folder . --scriptId 1m11ZhZl2TxlXkNKwvSfhqkO57ZBgu_KdfLUYIV1Lzksa64kiqQ32x5xA && clasp push",
    "pull": "npx clasp-env --folder . --scriptId 1m11ZhZl2TxlXkNKwvSfhqkO57ZBgu_KdfLUYIV1Lzksa64kiqQ32x5xA && clasp pull",
    "testAll": "npm run mochatest -- test/*.ts",
    "mochatest": "npx mocha --require ts-node/register --require test/hooks.ts",
    "tmem": "npm run mochatest -- test/testMember.ts",
    "ten": "npm run mochatest -- test/testEmailNotifier.ts",
    "ttp": "npm run mochatest -- test/testTransactionProcessor.ts",
    "tep": "npm run mochatest -- test/testExpirationProcessor.ts",
    "tn": "npm run mochatest -- test/testNotifier.ts",
    "run-test": "npm run mochatest --",
    "coverage": "npx nyc npm run testAll",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TobyHFerguson/SCCCCManagement.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/google-apps-script": "^1.0.82",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "chai": "^4.4.1",
    "gts": "^5.3.0",
    "mocha": "^10.4.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.4"
  }
}
