{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "noImplicitAny": false,
        "target": "es2015",
        "downlevelIteration": true,
        "typeRoots": [
            "./node_modules/@types",
            "./"
        ],
        "types": [
            "google-apps-script",
            "jest",
            "node"
        ],
        "lib": [
            "es2015",
            "es2021"
        ]
    },
    "include": [
        "**/*.js",
        "**/*.d.ts",
        "jest-globals.d.ts"
    ]
}